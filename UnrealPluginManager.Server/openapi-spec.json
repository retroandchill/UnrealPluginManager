{
  "openapi": "3.0.1",
  "info": {
    "title": "UnrealPluginManager.Server",
    "version": "1.0"
  },
  "paths": {
    "/api/plugins": {
      "get": {
        "tags": [
          "Plugins"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": {
              "minimum": 1,
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          },
          {
            "name": "size",
            "in": "query",
            "schema": {
              "maximum": 100,
              "minimum": 1,
              "type": "integer",
              "format": "int32",
              "default": 10
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PluginSummaryPage"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Plugins"
        ],
        "parameters": [
          {
            "name": "engineVersion",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "pluginFile": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "pluginFile": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PluginSummary"
                }
              }
            }
          }
        }
      }
    },
    "/api/plugins/{pluginName}": {
      "get": {
        "tags": [
          "Plugins"
        ],
        "parameters": [
          {
            "name": "pluginName",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/PluginSummary"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/plugins/{pluginName}/download": {
      "get": {
        "tags": [
          "Plugins"
        ],
        "parameters": [
          {
            "name": "pluginName",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "engineVersion",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/zip": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PluginSummary": {
        "required": [
          "name",
          "version"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "version": {
            "pattern": "^(?:0|[1-9]\\d*)\\.(?:0|[1-9]\\d*)\\.(?:0|[1-9]\\d*)(?:-(?:(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\\.(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\\+(?:[0-9a-zA-Z-]+(?:\\.[0-9a-zA-Z-]+)*))?$",
            "type": "string",
            "example": "1.0.0"
          },
          "description": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PluginSummaryPage": {
        "type": "object",
        "properties": {
          "pageNumber": {
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "totalPages": {
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "pageSize": {
            "minimum": 1,
            "type": "integer",
            "format": "int32",
            "example": 10
          },
          "count": {
            "minimum": 0,
            "type": "integer",
            "format": "int32",
            "example": 1
          },
          "item": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PluginSummary"
            }
          }
        }
      }
    }
  }
}